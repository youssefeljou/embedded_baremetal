#ifndef ADC_H_
#define ADC_H_

#include "std_types.h"

/* DEFINATION */
#define ADC_MAXIMUM_VALUE           1023
#define ADC_REF_VOLT_VALUE          2.56
#define AREF 0x00
#define AVCC 0x40
#define INTERNAL 0xC0
/*declare*/
#define F_CPU2  0x00
#define F_CPU4  0x02
#define F_CPU8  0x03
#define F_CPU16 0x04
#define F_CPU32 0x05
#define F_CPU64 0x06
#define F_CPU128 0x07




/*prototypes*/
void ADC_init(uint16 ref,uint16 prescaler)
uint16 ADC_readChannel(uint8 ch_num);

#endif /* ADC_H_ */
